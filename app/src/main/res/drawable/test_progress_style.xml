<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="270"
                android:centerY="1.0"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerY="1.0"
                    android:endColor="#1BE497"
                    android:startColor="#1BE497" />
            </shape>
        </clip>
    </item>


</layer-list>
